@model Vidly.Models.Movie
@{
    ViewBag.Title = "Get";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>@Model.Name</h1>
<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>Release Date: @Model.ReleaseDate.Date.ToString("dd.MM.yyyy")</li>
    <li>DateAdded: @Model.DateAdded.Date.ToString("yy.MM.dd")</li>
    <li>Number in Stock: @Model.NumberInStock.ToString() </li>
</ul>

